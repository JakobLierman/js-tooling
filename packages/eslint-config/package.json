{
  "name": "@jakoblierman/eslint-config",
  "description": "Personal ESLint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "version": "1.1.3",
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "default": "./dist/nextjs.js"
    },
    "./turborepo": {
      "types": "./dist/turborepo.d.ts",
      "default": "./dist/turborepo.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./jest": {
      "types": "./dist/jest.d.ts",
      "default": "./dist/jest.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "~4.6.0",
    "@eslint/eslintrc": "~3.3.3",
    "@eslint/js": "~10.0.1",
    "@next/eslint-plugin-next": "~16.1.6",
    "@stylistic/eslint-plugin": "~5.9.0",
    "@vitest/eslint-plugin": "~1.6.9",
    "eslint-config-prettier": "~10.1.8",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jest": "~29.15.0",
    "eslint-plugin-jsdoc": "~62.7.0",
    "eslint-plugin-n": "~17.24.0",
    "eslint-plugin-prettier": "~5.5.5",
    "eslint-plugin-react": "~7.37.5",
    "eslint-plugin-react-hooks": "~7.0.1",
    "eslint-plugin-regexp": "~3.0.0",
    "eslint-plugin-turbo": "~2.8.10",
    "eslint-plugin-unicorn": "~63.0.0",
    "eslint-plugin-unused-imports": "~4.4.1",
    "typescript-eslint": "~8.56.0"
  },
  "devDependencies": {
    "@jakoblierman/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24",
    "eslint": "^10.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^10.0.1"
  }
}
