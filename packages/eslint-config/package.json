{
  "name": "@jakoblierman/eslint-config",
  "description": "Personal ESLint configuration",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "version": "0.14.3",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "default": "./dist/nextjs.js"
    },
    "./turborepo": {
      "types": "./dist/turborepo.d.ts",
      "default": "./dist/turborepo.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./jest": {
      "types": "./dist/jest.d.ts",
      "default": "./dist/jest.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "~4.5.0",
    "@eslint/eslintrc": "~3.3.1",
    "@eslint/js": "~9.36.0",
    "@next/eslint-plugin-next": "~15.5.3",
    "@stylistic/eslint-plugin": "~5.4.0",
    "@vitest/eslint-plugin": "~1.3.12",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jest": "~29.0.1",
    "eslint-plugin-jsdoc": "~60.2.0",
    "eslint-plugin-n": "~17.23.1",
    "eslint-plugin-prettier": "~5.5.4",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-regexp": "~2.10.0",
    "eslint-plugin-turbo": "~2.5.7",
    "eslint-plugin-unicorn": "~61.0.2",
    "eslint-plugin-unused-imports": "~4.2.0",
    "typescript-eslint": "~8.44.1"
  },
  "devDependencies": {
    "@jakoblierman/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22",
    "eslint": "^9.36.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": "^9.36.0"
  }
}
